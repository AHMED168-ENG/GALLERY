        <!-- Recent Transactions -->
        <div class="row">
          <div id="recent-transactions" class="col-12">
            <div class="card">
              <div class="card-header">
                <h4 class="card-title">Recent Orders</h4>
                <a class="heading-elements-toggle"><i class="la la-ellipsis-v font-medium-3"></i></a>
                <div class="heading-elements">
                  <ul class="list-inline mb-0">
                    <li><a class="btn btn-sm btn-success box-shadow-2 round btn-min-width pull-right"
                      href="/dashboard/orders/" target="_blank">All Orders</a></li>
                  </ul>
                </div>
              </div>
              <div class="card-content">
                <div class="table-responsive">
                  <table id="recent-orders" class="table table-hover table-xl mb-0">
                    <thead>
                      <tr>
                        <th class="border-top-0">order Number</th>
                        <th class="border-top-0">Status</th>
                        <th class="border-top-0">Invoice#</th>
                        <th class="border-top-0">Customer Name</th>
                        <th class="border-top-0">Products</th>
                        <th class="border-top-0">Shipping</th>
                        <th class="border-top-0">Structure</th>
                        <th class="border-top-0">final price</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% orders.forEach((element , index) => { %>
                        
                        <tr>
                        <td class="text-truncate"><i class="la la-dot-circle-o <%= index % 2 != 0 ? "success" : "danger" %> font-medium-1 mr-1"></i>                          
                          <a style="color:#222;text-decoration: underline;" href="/dashboard/orders/<%= element.id  %>"> #<%= element.id %></a></td>
                        <td class="text-truncate"><i class="la la-dot-circle-o <%= element.isFinished ? "success" : "danger" %> font-medium-1 mr-1"></i>                          
                          <%= element.isFinished ? "Finished" : "Not Finished" %></td>
                        <td class="text-truncate"><a href="/download-order-pdf/<%= element.id %>"><%= "INV_" + element.id  %></a></td>
                        <td class="text-truncate">
                          <span class="avatar avatar-xs">
                            <img class="box-shadow-2" src="/assets/dashboard/Users/<%= element.orderUser.image.split("--")[0] %>"
                            alt="avatar">
                          </span>
                          <span><a href="/dashboard/users/update/<%= element.orderUser.id %>"><small><%= element.orderUser.fName + " " + element.orderUser.lName %></small></a></span>
                        </td>
                        <td class="text-truncate p-1">
                          <ul class="list-unstyled users-list m-0">
                            <% element.productOrderTable.slice(0 , 5).forEach(element1 => { %>
                              <li data-toggle="tooltip" data-popup="tooltip-custom" data-original-title="<%= element1.productTable.productName_en %>"
                            class="avatar avatar-sm pull-up">
                              <a href="/dashboard/products/update/<%= element1.productTable.id %>">
                                <img class="media-object rounded-circle no-border-top-radius no-border-bottom-radius"
                                src="/assets/dashboard/Products/<%= element1.productTable.productImage.split("--")[0] %>"
                                alt="Avatar">
                              </a>
                            </li>
                            <% }) %>
                            <li class="avatar avatar-sm">
                              <span class="badge badge-info">+<%= element.productOrderTable.length - 5 > 0 ? element.productOrderTable.length - 5 : 0 %> more</span>
                            </li>
                          </ul>
                        </td>
                       
                        <td>
                          <%= finalPriceForAdmin(element.productOrderTable).shipping %>Eg
                        </td>
                        <td class="text-truncate"><%= finalPriceForAdmin(element.productOrderTable).structure %>Eg</td>
                        <td class="text-truncate"><%= finalPriceForAdmin(element.productOrderTable).totalPrice %>Eg</td>
                      </tr>

                        <% }) %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
